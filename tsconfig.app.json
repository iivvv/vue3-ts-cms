{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      // "@/*": ["./src/*"]
      "@/*": ["src/*"] //？？为什么不报错了 node.json就不能省略./
    },
    "noImplicitAny": true //默认不允许
  }
}
